{
  "extends": "airbnb-base",
  "rules": {
    "func-names": ["error", "never"],
    "consistent-return": [
      0,
      {
        "treatUndefinedAsUnspecified": true
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "no-console": "off"
  }
}